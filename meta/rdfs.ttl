@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms-activity: <https://example.org/terms/activity/> .
@prefix terms-cmip: <https://example.org/terms/cmip/> .
@prefix terms-data: <https://example.org/terms/data/> .
@prefix terms-designing: <https://example.org/terms/designing/> .
@prefix terms-drs: <https://example.org/terms/drs/> .
@prefix terms-iso: <https://example.org/terms/iso/> .
@prefix terms-platform: <https://example.org/terms/platform/> .
@prefix terms-science: <https://example.org/terms/science/> .
@prefix terms-shared: <https://example.org/terms/shared/> .
@prefix terms-software: <https://example.org/terms/software/> .
@prefix terms-time: <https://example.org/terms/time/> .

terms-activity:Activity a rdfs:Class ;
    rdfs:label "Activity" ;
    rdfs:comment "Base class for activities." .

terms-activity:AxisMember a rdfs:Class ;
    rdfs:label "Axis Member" ;
    rdfs:comment """Description of a given ensemble member.

    It will normally be related to a specific ensemble requirement. Note
    that start dates can be extracted directly from the simulations and
    do not need to be recorded with an axis member description.

    """ .

terms-activity:ChildSimulation a rdfs:Class ;
    rdfs:label "Child Simulation" ;
    rdfs:comment "Defines the relationship between a simulation and its parent." ;
    rdfs:subclassOf "terms-activity:Simulation" .

terms-activity:Conformance a rdfs:Class ;
    rdfs:label "Conformance" ;
    rdfs:comment """A specific conformance.

    Describes how a particular numerical requirement has been
    implemented.  Will normally be linked from an ensemble descriptor.

    """ ;
    rdfs:subclassOf "terms-activity:Activity" .

terms-activity:ConformanceType a rdfs:Class ;
    rdfs:label "Conformance Type" ;
    rdfs:comment "Standardised set of conformance responses." .

terms-activity:Ensemble a rdfs:Class ;
    rdfs:label "Ensemble" ;
    rdfs:comment """Generic ensemble definition.

    Holds the definition of how the various ensemble members have been
    configured. If ensemble axes are not present, then this is either a
    single member ensemble, or part of an uber ensemble.

    """ ;
    rdfs:subclassOf "terms-activity:Activity" .

terms-activity:EnsembleAxis a rdfs:Class ;
    rdfs:label "Ensemble Axis" ;
    rdfs:comment "Defines the meaning of r/i/p indices in an ensemble." .

terms-activity:Simulation a rdfs:Class ;
    rdfs:label "Simulation" ;
    rdfs:comment """Simulation class provides the integrating link about what models
    were run and wny.""" ;
    rdfs:subclassOf "terms-iso:ProcessStep" .

terms-activity:UberEnsemble a rdfs:Class ;
    rdfs:label "Uber Ensemble" ;
    rdfs:comment """An ensemble made up of other ensembles.

    Often used where parts of an ensemble were run by different
    institutes. Could also be used when a new experiment is designed
    which can use ensemble members from previous experiments and/or
    projects.

    """ ;
    rdfs:subclassOf "terms-activity:Ensemble" .

terms-cmip:CmipDataset a rdfs:Class ;
    rdfs:label "Cmip Dataset" ;
    rdfs:comment "A CMIP dataset." ;
    rdfs:subclassOf "terms-data:Dataset" .

terms-cmip:CmipSimulation a rdfs:Class ;
    rdfs:label "Cmip Simulation" ;
    rdfs:comment """A CMIP simulation.

    In most CMIP cases this should be auto-generated from output dataset
    file headers.

    """ ;
    rdfs:subclassOf "terms-activity:Simulation" .

terms-data:Dataset a rdfs:Class ;
    rdfs:label "Dataset" ;
    rdfs:comment """Dataset discovery information.

    This may be further enhanced for ISO (or any other) compliance via
    the extra attributes or project specific sub-classing.

    """ .

terms-data:DatasetType a rdfs:Class ;
    rdfs:label "Dataset Type" ;
    rdfs:comment """Classifier of dataset type, to inform discovery metadata.

    Informed by Bedia et al,
    https://doi.org/10.1016/j.envsoft.2019.07.005, but adjusted for more
    generality.

    """ .

terms-data:VariableCollection a rdfs:Class ;
    rdfs:label "Variable Collection" ;
    rdfs:comment """A collection of variables within the scope of a code or process
    element.""" .

terms-designing:DomainRequirements a rdfs:Class ;
    rdfs:label "Domain Requirements" ;
    rdfs:comment """Properties of the domain which needs to be simulated, extent
    and/or resolution.""" ;
    rdfs:subclassOf "terms-designing:NumericalRequirement" .

terms-designing:EnsembleRequirement a rdfs:Class ;
    rdfs:label "Ensemble Requirement" ;
    rdfs:comment "Defines an experiment ensemble." ;
    rdfs:subclassOf "terms-designing:NumericalRequirement" .

terms-designing:EnsembleType a rdfs:Class ;
    rdfs:label "Ensemble Type" ;
    rdfs:comment """Defines the various axes along which one can set up an ensemble,
    whether as an experiment designer, or in designing a 'response'
    ensemble around an experiment.""" .

terms-designing:ExperimentalRelationship a rdfs:Class ;
    rdfs:label "Experimental Relationship" ;
    rdfs:comment "Defines the canonical set of experimental relationships." .

terms-designing:ForcingConstraint a rdfs:Class ;
    rdfs:label "Forcing Constraint" ;
    rdfs:comment "Identifies a model forcing constraint." ;
    rdfs:subclassOf "terms-designing:NumericalRequirement" .

terms-designing:ForcingType a rdfs:Class ;
    rdfs:label "Forcing Type" ;
    rdfs:comment """Defines the possible set of forcing types for a forcing
    constraint.""" .

terms-designing:InitialisationRequirement a rdfs:Class ;
    rdfs:label "Initialisation Requirement" ;
    rdfs:comment """A requirement on how a particular simulation should be
    initialised.""" ;
    rdfs:subclassOf "terms-designing:NumericalRequirement" .

terms-designing:MultiEnsemble a rdfs:Class ;
    rdfs:label "Multi Ensemble" ;
    rdfs:comment """In the case of multiple ensemble axes, defines how they are set
    up and ordered.""" ;
    rdfs:subclassOf "terms-designing:NumericalRequirement" .

terms-designing:NumericalExperiment a rdfs:Class ;
    rdfs:label "Numerical Experiment" ;
    rdfs:comment "Defines a numerical experiment." ;
    rdfs:subclassOf "terms-activity:Activity" .

terms-designing:NumericalRequirement a rdfs:Class ;
    rdfs:label "Numerical Requirement" ;
    rdfs:comment """A numerical requirement associated with a numerical
    experiment.""" ;
    rdfs:subclassOf "terms-activity:Activity" .

terms-designing:NumericalRequirementScope a rdfs:Class ;
    rdfs:label "Numerical Requirement Scope" ;
    rdfs:comment """The scope to which a numerical requirement may or may not
    apply.""" .

terms-designing:Objective a rdfs:Class ;
    rdfs:label "Objective" ;
    rdfs:comment """Describes a specific scientific objective within a project, and
    any necessary outputs from the experiment needed to meet this
    objective.""" .

terms-designing:OutputRequirement a rdfs:Class ;
    rdfs:label "Output Requirement" ;
    rdfs:comment """Provides details of what output is required and when from an
    experiment.""" ;
    rdfs:subclassOf "terms-designing:NumericalRequirement" .

terms-designing:Project a rdfs:Class ;
    rdfs:label "Project" ;
    rdfs:comment "Describes a scientific project." ;
    rdfs:subclassOf "terms-activity:Activity" .

terms-designing:SimulationPlan a rdfs:Class ;
    rdfs:label "Simulation Plan" ;
    rdfs:comment "Describes a simulation that needs to be run." ;
    rdfs:subclassOf "terms-activity:Activity" .

terms-designing:StartDateEnsemble a rdfs:Class ;
    rdfs:label "Start Date Ensemble" ;
    rdfs:comment "Defines an experiment ensemble with multiple start dates." ;
    rdfs:subclassOf "terms-designing:NumericalRequirement" .

terms-designing:TemporalConstraint a rdfs:Class ;
    rdfs:label "Temporal Constraint" ;
    rdfs:comment "A spatio-temporal constraint on a numerical experiment." ;
    rdfs:subclassOf "terms-designing:NumericalRequirement" .

terms-drs:DrsAtomicDataset a rdfs:Class ;
    rdfs:label "Drs Atomic Dataset" ;
    rdfs:comment "An entity in a DRS file system." .

terms-drs:DrsEnsembleIdentifier a rdfs:Class ;
    rdfs:label "Drs Ensemble Identifier" ;
    rdfs:comment """Identifies a 'response ensemble' realisation using the semantic
    content ofa 'run_variant_id'.""" .

terms-drs:DrsExperiment a rdfs:Class ;
    rdfs:label "Drs Experiment" ;
    rdfs:comment "An encoding of a drs experiment_id." .

terms-drs:DrsFrequencyType a rdfs:Class ;
    rdfs:label "Drs Frequency Type" ;
    rdfs:comment "Set of allowed DRS frequency types." .

terms-drs:DrsGeographicalIndicator a rdfs:Class ;
    rdfs:label "Drs Geographical Indicator" ;
    rdfs:comment """Specifies geographical subsets described by bounding boxes or by
    named regions.

    One of spatial domain or bounding box must appear.

    """ .

terms-drs:DrsGeographicalOperator a rdfs:Class ;
    rdfs:label "Drs Geographical Operator" ;
    rdfs:comment """Set of permitted spatial averaging operator suffixes for drs
    spatial indicators (yyyy-zzzz).""" .

terms-drs:DrsPublicationDataset a rdfs:Class ;
    rdfs:label "Drs Publication Dataset" ;
    rdfs:comment "PLACEHOLDER for the real drs_publication_dataset." .

terms-drs:DrsRealm a rdfs:Class ;
    rdfs:label "Drs Realm" ;
    rdfs:comment "Set of allowed DRS modelling realms." .

terms-drs:DrsSimulationIdentifier a rdfs:Class ;
    rdfs:label "Drs Simulation Identifier" ;
    rdfs:comment """That part of the DRS which identifies the response to the
    experiment: the simulation.""" .

terms-drs:DrsTemporalIdentifier a rdfs:Class ;
    rdfs:label "Drs Temporal Identifier" ;
    rdfs:comment """Provides information about temporal subsetting and/or averaging.

    If only N1 is present, it a temporal instant, If N1-N2 are present
    with no suffix, it is a temporal subset, If N1-N2 with a suffix are
    present, then some sort of temporal averaging has been applied
    across the period.

    """ .

terms-drs:DrsTimeSuffixe a rdfs:Class ;
    rdfs:label "Drs Time Suffixe" ;
    rdfs:comment """Set of permitted time averaging suffixes for drs temporal
    identifiers.""" .

terms-iso:Algorithm a rdfs:Class ;
    rdfs:label "Algorithm" ;
    rdfs:comment "Representation of the LE_Algorithm Class." .

terms-iso:DqEvaluationResultType a rdfs:Class ;
    rdfs:label "Dq Evaluation Result Type" ;
    rdfs:comment "Classifier of evaluation results." .

terms-iso:DsInitiativeTypecode a rdfs:Class ;
    rdfs:label "Ds Initiative Typecode" ;
    rdfs:comment """Classifier of initiative, to inform ISO19115 metadata.

    Formally a DS_InitiativeTypeCode, from ISO19115_2011.

    """ .

terms-iso:Lineage a rdfs:Class ;
    rdfs:label "Lineage" ;
    rdfs:comment """Representation of the ISO19115 lineage provenance description.

    Information about the events or source data used in constructing a
    dataset

    """ .

terms-iso:MdCellgeometryCode a rdfs:Class ;
    rdfs:label "Md Cellgeometry Code" ;
    rdfs:comment """Classifier of cells.

    Whether a grid point is point or area.

    """ .

terms-iso:MdProgressCode a rdfs:Class ;
    rdfs:label "Md Progress Code" ;
    rdfs:comment "Classifier of project or dataset progress." .

terms-iso:ProcessStep a rdfs:Class ;
    rdfs:label "Process Step" ;
    rdfs:comment """Representation of the ISO19115 LE_ProcessStep and parent
    LI_ProcessStep classes.""" .

terms-iso:ProcessStepReport a rdfs:Class ;
    rdfs:label "Process Step Report" ;
    rdfs:comment """Report of what happened during a processing step.

    Representation of ISO LE_ProcessStepReport, modified to use links or
    body text, rather than files.

    """ .

terms-iso:Processing a rdfs:Class ;
    rdfs:label "Processing" ;
    rdfs:comment """Representation of the ISO19115 LE_Processing class Note that the
    algorithm definition has been adjusted to be more generic and less
    "instrument obsessed" than ISO19115.

    Name is an extension, and the identifier is simply a code string
    (id) ... but given there may be no identifier space for this
    processing step, it is made optional, rather than mandatory as in
    ISO. For export to ISO, the recommendation is to use the identifier
    of the CIM document which uses this class.

    """ .

terms-iso:QualityEvaluationOutput a rdfs:Class ;
    rdfs:label "Quality Evaluation Output" ;
    rdfs:comment "A specific evaluation output." ;
    rdfs:subclassOf "terms-shared:OnlineResource" .

terms-iso:QualityEvaluationResult a rdfs:Class ;
    rdfs:label "Quality Evaluation Result" ;
    rdfs:comment """The output of some quality evaluation against a specific measure
    for evaluation quality.

    This flattens several ISO classes, including DQ_Result,
    DQ_ConformanceResult, DQ_QuantativeResult, DQ_Element.

    """ .

terms-iso:QualityIssue a rdfs:Class ;
    rdfs:label "Quality Issue" ;
    rdfs:comment """A description of some scientific quality issue known about a
    resource described by this ontology.

    E.g. if a model is known to have a particular problem, or there has
    been a problem found with a dataset. Expect that most such detail
    should be managed in an external (and formal) issue tracker.

    """ .

terms-iso:QualityReport a rdfs:Class ;
    rdfs:label "Quality Report" ;
    rdfs:comment """A report detailing the quality of some aspect of the target resource.
    
    """ .

terms-platform:ComputePool a rdfs:Class ;
    rdfs:label "Compute Pool" ;
    rdfs:comment "Homogeneous pool of nodes within a computing machine." .

terms-platform:Interconnect a rdfs:Class ;
    rdfs:label "Interconnect" ;
    rdfs:comment "The interconnect used within a machine to join nodes together." .

terms-platform:Machine a rdfs:Class ;
    rdfs:label "Machine" ;
    rdfs:comment """A computer/system/platform/machine which is used for
    simulation.""" ;
    rdfs:subclassOf "terms-platform:Partition" .

terms-platform:Nic a rdfs:Class ;
    rdfs:label "Nic" ;
    rdfs:comment "Network Interface Card." .

terms-platform:Partition a rdfs:Class ;
    rdfs:label "Partition" ;
    rdfs:comment """A major partition (component) of a computing system (aka
    machine).""" .

terms-platform:Performance a rdfs:Class ;
    rdfs:label "Performance" ;
    rdfs:comment """Describes the properties of a performance of a configured model
    on a particular system/machine.

    Based on "CPMIP: Measurements of Real Computational Performance of
    Earth System Models" (Balaji et. al. 2016,
    doi:10.5194/gmd-2016-197).

    """ .

terms-platform:PerformanceDetail a rdfs:Class ;
    rdfs:label "Performance Detail" ;
    rdfs:comment """Information about how the various components of performance were
    related.""" .

terms-platform:ProjectCost a rdfs:Class ;
    rdfs:label "Project Cost" ;
    rdfs:comment "Cost of an experiment or project on a particular platform." .

terms-platform:StoragePool a rdfs:Class ;
    rdfs:label "Storage Pool" ;
    rdfs:comment "Homogeneous storage pool on a computing machine." .

terms-platform:StorageSystem a rdfs:Class ;
    rdfs:label "Storage System" ;
    rdfs:comment """Controlled vocabulary for storage types (including
    filesystems).""" .

terms-science:Model a rdfs:Class ;
    rdfs:label "Model" ;
    rdfs:comment """A model component: can be executed standalone, and has as
    scientific description available via a link to a science.domain
    document.

    (A configured model can be understood in terms of a simulation, a
    model, and a configuration.).

    """ ;
    rdfs:subclassOf "terms-software:ComponentBase" .

terms-science:ModelType a rdfs:Class ;
    rdfs:label "Model Type" ;
    rdfs:comment """Defines a set of model types relevant to weather, climate, and
    earth system modelling.""" .

terms-science:Realm a rdfs:Class ;
    rdfs:label "Realm" ;
    rdfs:comment """Scientific area of a numerical model - usually a sub-model or
    component.""" ;
    rdfs:subclassOf "terms-science:Topic" .

terms-science:RealmCoupling a rdfs:Class ;
    rdfs:label "Realm Coupling" ;
    rdfs:comment "Description of a coupling between realms." .

terms-science:Topic a rdfs:Class ;
    rdfs:label "Topic" ;
    rdfs:comment """An organized collection of details upon a specific topic, e.g.
    model key properties.""" .

terms-science:TopicProperty a rdfs:Class ;
    rdfs:label "Topic Property" ;
    rdfs:comment "A specialized question asked of the scientic community." .

terms-science:TopicPropertySet a rdfs:Class ;
    rdfs:label "Topic Property Set" ;
    rdfs:comment """Provides specific details related to a topic (i.e. process, sub-
    process, grid, key properties, etc).""" .

terms-shared:Citation a rdfs:Class ;
    rdfs:label "Citation" ;
    rdfs:comment "An academic reference to published work." .

terms-shared:DocMetaInfo a rdfs:Class ;
    rdfs:label "Doc Meta Info" ;
    rdfs:comment """Encapsulates document meta information used by es-doc machinery.

    Will not normally be populated by humans. May duplicate information
    held in 'visible' metadata.

    """ .

terms-shared:DocReference a rdfs:Class ;
    rdfs:label "Doc Reference" ;
    rdfs:comment "A reference to another cim entity." .

terms-shared:ExtraAttribute a rdfs:Class ;
    rdfs:label "Extra Attribute" ;
    rdfs:comment """An extra attribute with key and value needed to encode further
    information not in the CIM domain model or specialisation.

    Typical use case: in parsing data and encoding attributes found in
    data.

    """ .

terms-shared:FormalAssociation a rdfs:Class ;
    rdfs:label "Formal Association" ;
    rdfs:comment """Holds a named association between entities, where the name of the
    association comes from a specific named enumeration.

    The association can point at a CIM entity, or a remote entity.

    """ ;
    rdfs:subclassOf "terms-shared:DocReference" .

terms-shared:NilReason a rdfs:Class ;
    rdfs:label "Nil Reason" ;
    rdfs:comment """Provides an enumeration of possible reasons why a property has
    not been defined Based on GML nilReason as discussed here:
    https://www.seegrid.csiro.au/wiki/AppSchemas/NilValues.""" .

terms-shared:Numeric a rdfs:Class ;
    rdfs:label "Numeric" ;
    rdfs:comment """A number which comes with a unit, potentially from a controlled
    vocabulary of units.

    #FIXME: Need to work on the relationship between unit_source and base_unit.

    """ .

terms-shared:OnlineResource a rdfs:Class ;
    rdfs:label "Online Resource" ;
    rdfs:comment """A minimal approximation of ISO19115 CI_ONLINERESOURCE, provides a
    link and details of how to use that link.""" .

terms-shared:Party a rdfs:Class ;
    rdfs:label "Party" ;
    rdfs:comment """Implements minimal material for an ISO19115-1 (2014) compliant
    party.

    For our purposes this is a much better animal than the previous
    responsibleParty which munged roles together with people. Note we
    have collapsed CI_Contact, CI_Individual and CI_Organisation as well
    as the abstract CI_Party.

    """ .

terms-shared:QualityReview a rdfs:Class ;
    rdfs:label "Quality Review" ;
    rdfs:comment """Assertions as to the completeness and quality of a document.

    Not to be confused with assertions as to the quality of the resource
    described by the document (as covered by the iso.quality_report). A
    future version of this ontology may rename this class.

    """ .

terms-shared:QualityStatus a rdfs:Class ;
    rdfs:label "Quality Status" ;
    rdfs:comment "Assertion as to the review status of document." .

terms-shared:Responsibility a rdfs:Class ;
    rdfs:label "Responsibility" ;
    rdfs:comment """Implements the ISO19115-1 (2014) CI_Responsibility (which
    replaces responsibleParty).

    Combines a person and their role in doing something.

    """ .

terms-shared:RoleCode a rdfs:Class ;
    rdfs:label "Role Code" ;
    rdfs:comment """Responsibility role codes: roles that a party may play in
    delivering a responsibility.

    This is an extension and modification of CI_RoleCode from ISO19115.

    """ .

terms-shared:TextBlob a rdfs:Class ;
    rdfs:label "Text Blob" ;
    rdfs:comment """Provides a text class which supports plaintext, html, and friends
    (or will do).""" .

terms-shared:TextBlobEncoding a rdfs:Class ;
    rdfs:label "Text Blob Encoding" ;
    rdfs:comment """Types of text understood by the CIM notebook.

    Currently only plaintext, but we expect safe HTML to be supported as
    soon as practicable.

    """ .

terms-software:ComponentBase a rdfs:Class ;
    rdfs:label "Component Base" ;
    rdfs:comment """Base class for software component properties, whether a top level
    model, or a specific piece of code known as a component.

    In software terms, a component is a discrete set of code that takes
    input data and generates output data. Components may or may not have
    scientific descriptions.

    """ .

terms-software:Composition a rdfs:Class ;
    rdfs:label "Composition" ;
    rdfs:comment """Describes how component variables are coupled together either
    to/from other SoftwareComponents or external data files. The
    variables specified by a component's composition must be owned by
    that component, or a  child of that component; child components
    cannot couple together parent variables.

    # FIXME: THIS CLASS IS BELIEVED TO BE OBSOLETE AND WILL BE
    REVIEWED/REPLACED IN THE NEXT VERSION

    """ .

terms-software:CouplingFramework a rdfs:Class ;
    rdfs:label "Coupling Framework" ;
    rdfs:comment "The set of terms which define known coupling frameworks." .

terms-software:DevelopmentPath a rdfs:Class ;
    rdfs:label "Development Path" ;
    rdfs:comment """Describes the software development path for this
    model/component.""" .

terms-software:EntryPoint a rdfs:Class ;
    rdfs:label "Entry Point" ;
    rdfs:comment """Describes a function or subroutine of a SoftwareComponent. BFG
    will use these EntryPoints to define a schedule of subroutine calls
    for a coupled model. Currently, a very basic schedule can be
    approximated by using the 'proceeds' and 'follows' attributes,
    however a more complete system is required for full BFG
    compatibility. Every EntryPoint can have a set of arguments
    associated with it. These reference (previously defined) variables.

    # FIXME: THIS CLASS IS BELIEVED TO BE OBSOLETE AND WILL BE
    REVIEWED/REPLACED IN THE NEXT VERSION

    """ .

terms-software:Gridspec a rdfs:Class ;
    rdfs:label "Gridspec" ;
    rdfs:comment """Fully defines the computational grid used.

    # FIXME: THIS CLASS IS BELIEVED TO BE OBSOLETE AND WILL BE
    REVIEWED/REPLACED IN THE NEXT VERSION

    """ .

terms-software:Implementation a rdfs:Class ;
    rdfs:label "Implementation" ;
    rdfs:comment """Implementation information for a software framework/component,
    whether a top level model, or a specific piece of code known as a
    'component'.

    In software terms, a software framework/component is a discrete set
    of code that takes input data and generates output data. Software
    frameworks/components may or may not have scientific descriptions.

    """ .

terms-software:ProgrammingLanguage a rdfs:Class ;
    rdfs:label "Programming Language" ;
    rdfs:comment """The set of terms which define programming languages used for
    earth system simulation.""" .

terms-software:SoftwareComponent a rdfs:Class ;
    rdfs:label "Software Component" ;
    rdfs:comment """An embedded piece of software that does not normally function as
    a standalone model (although it may be used standalone in a test
    harness).""" ;
    rdfs:subclassOf "terms-software:ComponentBase" .

terms-software:Variable a rdfs:Class ;
    rdfs:label "Variable" ;
    rdfs:comment """An instance of a model software variable which may be prognostic
    or diagnostic, and which is available as a connection to other
    software components. Note that these variables may only exist within
    the software workflow as interim quantities or coupling endpoints.
    Input and output variables will be a subset of these software
    variables.

    # FIXME: THIS CLASS IS BELIEVED TO BE OBSOLETE AND WILL BE
    REVIEWED/REPLACED IN THE NEXT VERSION

    """ .

terms-time:Calendar a rdfs:Class ;
    rdfs:label "Calendar" ;
    rdfs:comment """Describes the calendar required/used in an experiment/simulation.

    This class is based on the calendar attributes and properties found
    in the CF netCDF conventions.

    """ .

terms-time:CalendarType a rdfs:Class ;
    rdfs:label "Calendar Type" ;
    rdfs:comment "CF calendar types as defined in CF 1.6." .

terms-time:DateTime a rdfs:Class ;
    rdfs:label "Date Time" ;
    rdfs:comment """A date or time.

    Either in simulation time with the simulation calendar, or with
    reference to a simulation start, in which case the datetime is an
    interval after the start date.

    """ .

terms-time:DatetimeSet a rdfs:Class ;
    rdfs:label "Datetime Set" ;
    rdfs:comment """A set of times.

    This is an abstract class which is specialised into a periodic or
    aperiodic (irregular) list.  Note that we assume either a periodic
    set of dates which can be date and/or time, or an irregular set
    which can only be dates.

    """ .

terms-time:IrregularDateset a rdfs:Class ;
    rdfs:label "Irregular Dateset" ;
    rdfs:comment """A set of irregularly spaced times, provided as a comma separated
    string of yyyy-mm-dd in the appropriate calendar.""" ;
    rdfs:subclassOf "terms-time:DatetimeSet" .

terms-time:PeriodDateType a rdfs:Class ;
    rdfs:label "Period Date Type" ;
    rdfs:comment "A period date type vocabulary (used by time_period)." .

terms-time:RegularTimeset a rdfs:Class ;
    rdfs:label "Regular Timeset" ;
    rdfs:comment "A regularly spaced set of times." ;
    rdfs:subclassOf "terms-time:DatetimeSet" .

terms-time:TimePeriod a rdfs:Class ;
    rdfs:label "Time Period" ;
    rdfs:comment "A time period/interval (aka temporal extent)." .

terms-time:TimeUnit a rdfs:Class ;
    rdfs:label "Time Unit" ;
    rdfs:comment """Appropriate Time units for experiment durations in NWP and
    Climate Modelling.""" .

terms-activity:Conformed a terms-activity:ConformanceType ;
    rdfs:label "Conformed" ;
    rdfs:comment "Simulation (or ensemble) conformed to requirement" .

terms-activity:alternativeName a rdfs:Property ;
    rdfs:label "alternative name" ;
    rdfs:comment "List of names by which the activity is also known." .

terms-activity:axis a rdfs:Property ;
    rdfs:label "axis" ;
    rdfs:comment "The parent axis of this ensemble member" .

terms-activity:branchMethod a rdfs:Property ;
    rdfs:label "branch method" ;
    rdfs:comment "Description of how the simulation was branched from a parent simulation, e.g. 'standard', 'none provided'." .

terms-activity:branchTimeInChild a rdfs:Property ;
    rdfs:label "branch time in child" ;
    rdfs:comment "The time at which the present simulation started in the child calendar." .

